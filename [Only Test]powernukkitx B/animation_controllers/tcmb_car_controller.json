        "controller.animation.tcmb_curve": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "pt0": "q.is_tamed==1"
                        },
                        {
                            "pt1": "q.can_fly==1"
                        },
                        {
                            "pt2": "q.can_power_jump==1"
                        }
                    ]
                },
                "pt0": {
                    "transitions": [
                        {
                            "default": "q.is_tamed!=1"
                        }
                    ],
                    "animations": [ "tcmb_pt0" ]
                },
                "pt1": {
                    "transitions": [
                        {
                            "default": "q.can_fly!=1"
                        }
                    ],
                    "animations": [ "tcmb_pt1" ]
                },
                "pt2": {
                    "transitions": [
                        {
                            "default": "q.can_power_jump!=1"
                        }
                    ],
                    "animations": [ "tcmb_pt2" ]
                }
            }
        },
        "controller.animation.tcmb_direction": {
            "states":{
                "default": {
                    "transitions":[
                        {
                            "x_plus": "q.movement_direction(0)>0"
                        },
                        {
                            "x_minus": "q.movement_direction(0)<0"
                        },
                        {
                            "z_plus": "q.movement_direction(2)>0"
                        },
                        {
                            "z_minus": "q.movement_direction(2)<0"
                        }
                    ]
                },
                "x_plus": {
                    "transitions":[
                        {
                            "default": "q.movement_direction(0)<=0"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add x_plus",
                        "v.direction = 1;"
                    ],
                    "on_exit": [
                        "/tag @s remove x_plus",
                        "v.direction = 0;"
                    ]
                },
                "x_minus": {
                    "transitions":[
                        {
                            "default": "q.movement_direction(0)>=0"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add x_minus",
                        "v.direction = 2;"
                    ],
                    "on_exit": [
                        "/tag @s remove x_minus",
                        "v.direction = 0;"
                    ]
                },
                "z_plus": {
                    "transitions":[
                        {
                            "default": "q.movement_direction(2)<=0"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add z_plus",
                        "v.direction = 3;"
                    ],
                    "on_exit": [
                        "/tag @s remove z_plus",
                        "v.direction = 0;"
                    ]
                },
                "z_minus": {
                    "transitions":[
                        {
                            "default": "q.movement_direction(2)>=0"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add z_minus",
                        "v.direction = 4;"
                    ],
                    "on_exit": [
                        "/tag @s remove z_minus",
                        "v.direction = 0;"
                    ]
                }
            }
        }
    }
